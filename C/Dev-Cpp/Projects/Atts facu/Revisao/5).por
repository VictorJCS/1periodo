programa
{
	
	funcao inicio()
	{
		real produto, produto_r
		
		escreva(" Informe o valor do produto: R$")
		leia(produto)
		
		produto_r = produto - (produto * 0.10)

		escreva(" O valor do produto com desconto e: R$",produto_r)
	}
}
