programa
{
	
	funcao inicio()
	{
		real num1, num2, div
		
		escreva(" Informe um numero: ")
		leia(num1)

		escreva(" Informe outro numero: ")
		leia(num2)
		
		div = num1 / num2

		escreva(" O resultado da subtracao do primeiro numero pelo segundo e: ", div)
		
		
	}
}

