programa
{
	
	funcao inicio()
	{
		real num1, num2, num3, mult

		escreva(" Digite um numero: ")
		leia(num1)

		escreva(" Digite outro numero: ")
		leia(num2)

		escreva(" Digite o ultimo numero: ")
		leia(num3)

		mult = num1 * num2 * num3 

		escreva(" O resultado da multiplicacao e: ", mult)
	}
}
