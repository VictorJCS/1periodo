programa
{
	
	funcao inicio()
	{
		real num1, num2, media

		escreva(" Digite um numero: ")
		leia(num1)

		escreva(" Digite outro numero: ")
		leia(num2)

		media = (num1 * 0.2 + num2 * 0.3)/(0.2 + 0.3)

		escreva(" A sua media e:: ",media)
	}
}
