programa
{
	
	funcao inicio()
	{
		inteiro num1, num2, sub
		
		escreva(" Digite um numero ")
		leia(num1)
		
		escreva(" Digite outro numero ")
		leia(num2)

		sub = num1 - num2 

		escreva("A subtracao dos valores digitados e ",sub)
	}
}
