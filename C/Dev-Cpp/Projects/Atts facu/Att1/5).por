programa
{
	
	funcao inicio()
	{
		real preco, preco_a

		escreva(" Digite o preco: ")
		leia(preco)

		preco_a = preco - (preco * 0.10) 

		escreva(" O preco com desconto e: ",preco_a)
	}
}
