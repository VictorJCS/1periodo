programa
{
	
	funcao inicio()
	{
		real num1, num2, div

		escreva(" Digite um numero: ")
		leia(num1)

		escreva(" Digite outro numero, diferente de zero: ")
		leia(num2)

		div = num1/num2

		escreva(" O resultado da divisao e: ", div)
	}
}
